# Frontend origin calling the API
FRONTEND_ORIGIN=http://localhost:5173

# Alibaba OAuth Configuration (REQUIRED - no mocks)
ALIBABA_CLIENT_ID=your_app_client_id
ALIBABA_CLIENT_SECRET=your_app_client_secret
# Optional in dev; computed from request if not set:
# ALIBABA_REDIRECT_URI=http://localhost:8000/api/integrations/alibaba/oauth/callback

# Alibaba API Configuration
ALIBABA_AUTH_BASE=https://signin.alibabacloud.com
ALIBABA_TOKEN_BASE=https://oauth.alibabacloud.com
ALIBABA_API_BASE=https://api.alibaba.com
ALIBABA_OAUTH_SCOPE=openid

# Alibaba B2B API Configuration for SLA Search Integration
FEATURE_ALIBABA=1
ALIBABA_B2B_CLIENT_ID=your_b2b_client_id
ALIBABA_B2B_CLIENT_SECRET=your_b2b_client_secret
ALIBABA_B2B_REDIRECT_URI=http://localhost:8000/api/alibaba/oauth/callback
ALIBABA_B2B_API_BASE=https://api.alibaba.com/

# Database
DATABASE_URL=sqlite:///./sla.db

# Redis (for background jobs)
REDIS_URL=redis://localhost:6379/0

# Encryption key for sensitive data
ENCRYPTION_KEY=your_32_character_encryption_key_here

# Environment
ENVIRONMENT=development
